<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_item_view"
    android:layout_width="wrap_content"
    android:layout_height="60dp"
    android:layout_margin="@dimen/ten_dp"
    android:background="@drawable/unselected"
    android:gravity="center"
    android:orientation="vertical">



    <LinearLayout

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:orientation="vertical"
        >

        <ImageView
            android:id="@+id/ivselectedVehicle"
            android:layout_width="12dp"
            android:layout_height="10dp"
            android:src="@drawable/ic_selected_vehicle"
            android:layout_marginStart="@dimen/five_dp"
            android:layout_marginLeft="@dimen/five_dp" />

        <com.walkins.technician.custom.NormalTextView
            android:id="@+id/ivVehicleImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="185/65 R15"
            android:paddingStart="@dimen/five_dp"
            android:paddingEnd="@dimen/five_dp"
            android:textColor="@color/text_color" />

    </LinearLayout>
</LinearLayout>
